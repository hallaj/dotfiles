!
! Hallaj ~/.XDefaults

! rxvt-unicode
URxvt.iconFile:         /usr/share/icons/hicolor/scalable/apps/terminator.svg
URxvt.termName:         rxvt-256color
URxvt.background:       #2e3436
URxvt.depth:            32
URxvt.scrollBar:        false
URxvt.inheritPixmap:    false
URxvt.tintColor:        white
URxvt.shading:          85
URxvt.geometry:         150x50
URxvt.buffered:         true
URxvt.borderLess:       true
URxvt.loginShell:       false
URxvt.font:             xft:Monospace:size=8
URxvt.boldFont:         xft:Monospace:size=8
URxvt.urlLauncher:      x-www-browser
URxvt.underlineURLs:    true
URxvt.perl-ext-common:  default,tabbed,matcher
URxvt.colorUL:          #4682B4
URxvt.letterSpace:      -1

! colorScheme
*.background:           rgb:00/00/00
*.foreground:           rgb:cf/cf/cf
*color0:                rgb:00/00/00
*color1:                rgb:e0/10/10
*color2:                rgb:20/ad/20
*color3:                rgb:d4/c2/4f
*color4:                rgb:23/1b/b8
*color5:                rgb:9c/38/85
*color6:                rgb:1d/bd/b8
*color7:                rgb:fe/fe/fe
*color8:                rgb:6a/6a/6a
*color9:                rgb:e8/3a/3d
*color10:               rgb:35/e9/56
*color11:               rgb:ff/ff/2f
*color12:               rgb:3a/53/f0
*color13:               rgb:e6/28/ba
*color14:               rgb:1c/f5/f5
*color15:               rgb:ff/ff/ff
